div,ul,li,table,tr,td,form,input,select,textarea,button,em,h1,h2{
	font-family:Arial, Verdana, Helvetica, sans-serif;
	font-size:11.2px;
	overflow:hidden;
	}
div,ul,li{
	list-style:none;
	margin:0px;
	padding:0px;	
	}
em{
	font-weight:bold;
	font-style:normal;
	}
fieldset{
	border:none; /*modified by pcui*/
	margin-top:5px;
	margin-bottom:5px;
	}
fieldset legend{
	color:#000;
	font-weight:bold;	
	}		
fieldset label{
	font-size:10pt; /*added by pcui*/
	display:block;
	margin-left:12px;
	}
fieldset input{
	font-size:10pt; /*added by pcui*/
	/*width:102px;*/
	background:none;	/*modified by pcui*/
	border:1px solid #E0DFE3; /*modified by pcui*/
	margin-left:12px;
	}	
fieldset button{
	font-size:10.4px;
	border:2px solid #AAA;
	border-color:#AAA #444 #444 #AAA;
	background:#FFFFE6;
	font-weight:bold;
	cursor:hand;
	
	margin-left:12px;
	margin-top:8px;
	display:block;
	clear:both;
	}
fieldset select{
	margin-left:12px;
	background:none; /*modified by pcui*/
	border:1px solid #E0DFE3;
	font-size:10pt; /*added by pcui*/
	
	margin-bottom:5px;
	}	
#forIEbug1{
	position:absolute;
	width:100%; 
	left:0px; 
	top:20px; 
	*top:0px;
	z-index:9;	
	}		
.messageAndRefresh{
	width:100%; 
	min-width:500px; 
	max-width:100%; 
	height:auto;
	}
.messageBox{
	width:79.5%; 
	height:auto; 
	float:left; 
	margin-right:0.5%;
	}
.messageContent{
	height:auto;
	min-height:28px; 
	padding:2px; 
	font-weight:bold; 
	background:#FFFAEB; 
	width:inherit; 
	border:1px solid #CCC;
	}
.messageContent h1,.messageContent h2{
	margin:0px;
	padding:0px;
	}
.messageContent h1{
	font-size:10.4px;
	color:#999;
	}	
.messageContent h1 a,.messageContent h2 a{
	color:#7da0c5;
}	
		
.refreshButtonAndTime{
	width:20%; 
	height:auto; 
	float:left;
	}
ul.refreshArea li{
	text-align:right;
	}					
.filterBaseArea{
	margin-top:5px;
	height:1px; 
	min-width:500px; 
	position:relative; 
	width:100%; 
	overflow:visible;
	}	
.filterRange{
	width:200px; 
	position:absolute; 
	top:1px; right:0px;
	}
		
.filterNoDisplay,.filterDisplay{
	width:198px; 
	height:auto;
	overflow:visible; 
	background:#FFF;
	border:1px solid #000;
	border-top:none;
	position:relative;		
	}
.filterInnerContent{
	position:absolute; 
	left:0px; 
	top:0px; 
	z-index:99;
	}	
.filterNoDisplay{
	display:none;
	}
.filterDisplay{
	display:block;
	}		
.filterSettingInfo{
	height:30px; 
	line-height:30px; 
	width:100%; 
	min-width:500px;
	}	
.filterEntranceClose,.filterEntranceOpen{
	width:198px; 
	background:#FFF; 
	border:1px solid #000;
	height:20px; 
	line-height:20px; 
	}
.filterEntranceOpen{	
	border-bottom:none;
	}	
.filterEntranceClose a.close,.filterEntranceClose a.open,.filterEntranceOpen a.close,.filterEntranceOpen a.open{
	background:url(../images/icon-close.gif) no-repeat 6px 1px;
	padding-left:20px;
	color:#00f;
	text-decoration:underline;
	}
.filterEntranceClose a.open,.filterEntranceOpen a.open{
	background:url(../images/icon-open.gif) no-repeat 6px 1px;
	}	
.filterEntranceClose a.close:hover,.filterEntranceClose a.open,.filterEntranceOpen a.close:hover,.filterEntranceOpen a.open{	
	color:#6565ff;
	}

.globalAction{
	margin-bottom:-5px;
	}	
.globalAction select{
	background:#FFFFE6;
	border:1px solid #999;
	}				
.buttonStyle1{
	font-size:10.4px;
	border:2px solid #AAA;
	border-color:#AAA #444 #444 #AAA;
	background:#FFFFE6;
	font-weight:bold;
	cursor:hand;
	}						
	
	
	
	
	
	
	
	
	
/* List-table CSS */
			
.tableNaviLeading-top,.tableNaviLeading-bottom{
	max-width:100%;
	/*width:1%;*/
	width:100%;
/**width:auto;*/	
	border-collapse:collapse;
	border-cellpadding:0px;	
	/*border:1px solid #7f7f7f;*/
	}
.forIE a.first,
.forIE a.previous,
.forIE a.next,
.forIE a.last{	
	color:#8d8d8f;
	text-decoration:underline;
	}
.forIE a.first:hover,
.forIE a.previous:hover,
.forIE a.next:hover,
.forIE a.last:hover{	
	color:#00f;
	text-decoration:underline;
	}	
.forIE a.first,
.forIE a.previous,
.forIE a.next,
.forIE a.last,
.forIE a.firstDisabled,
.forIE a.previousDisabled,
.forIE a.nextDisabled,
.forIE a.lastDisabled{
	padding-left:15px;
	background:url(../images/icon-nav-first.gif) no-repeat 0px 3px;
	}
.forIE a.previous{
	background:url(../images/icon-nav-previous.gif) no-repeat 0px 3px;
	}
.forIE a.next{
	padding:0px;
	padding-right:15px;
	background:url(../images/icon-nav-next.gif) no-repeat right 3px;
	}		
.forIE a.last{
	padding:0px;
	padding-right:15px;
	background:url(../images/icon-nav-last.gif) no-repeat right 3px;
	}
.forIE a.firstDisabled,
.forIE a.previousDisabled,
.forIE a.nextDisabled,
.forIE a.lastDisabled,
{
	color:#d7d7d9;
	text-decoration:none;
}		
/*disabled links*/		
.forIE a.firstDisabled{
	background:url(../images/icon-nav-first-dim.gif) no-repeat 0px 3px;	
	}			
.forIE a.previousDisabled{
	background:url(../images/icon-nav-previous-dim.gif) no-repeat 0px 3px;	
	}			
.forIE a.nextDisabled{
	padding:0px;
	padding-right:15px;
	background:url(../images/icon-nav-next-dim.gif) no-repeat right 3px;	
	}			
.forIE a.lastDisabled{
	padding:0px;
	padding-right:15px;
	background:url(../images/icon-nav-last-dim.gif) no-repeat right 3px;	
	}			
.forIE{
/**width:800px;*/	
	}	
.preferences{	
	width:100%; 
	min-width:500px;
	height:16px; 
	/*border-bottom:2px solid #666;*/ 
	/*background:#fffaeb;*/ 
	text-align:right;
	}	
.needBold{
	color:#666;
	font-weight:bold;
	}	
.tableNaviLeading-bottom{
	border-top-width:1px;
	border-bottom-width:2px; 
	}	
		
.tableBasic{
	max-width:100%;
	width:100%;
	border-collapse:collapse;
	border-cellpadding:0px;	
	border-left:1px solid #7f7f7f;	
	border-right:1px solid #7f7f7f; 	
	border-bottom:1px solid #7f7f7f;
*border:1px solid #7f7f7f;	
*border-top-width:0px; 	
	}
.tableBasic th,td{
	font-size:10pt;
	padding:5px;
	word-break:keep-all;
	/*white-space:nowrap;*/
	}
.tableBasic th{
	font-size:11pt;
	font-weight:normal;
	color:#036;
	white-space:nowrap;
	}
.tableBasic td div{
    font-size:10pt;
    font-family:Arial, Verdana, Helvetica, sans-serif;
	}
.tableBasic th,.tableBasic th.highLight{
	padding:0px;
	color:#036;
	text-align:center;
	border-top:1px solid #7f7f7f;
	border-right:1px solid #cbcbcb;
	background:#d8d8da url(../images/ThBg.png) repeat-x;		
	}
.tableBasic th.highLight{
	background:#a5a8ab url(../images/ThBgSelected.png) repeat-x;	
	}		
.tdRightAlign{
	text-align:right;
	}
th.thRightAlign,td.tdRightAlign{
	text-align:right;
	}
th.thCenterAlign,td.tdCenterAlign{
	text-align:center
	}
th.thLeftAlign,td.tdLeftAlign{
	text-align:left;
	}

		
.tableBasic th div,.tableBasic th div.sortCurrentAscend,.tableBasic th div.sortCurrentDescend{	
	padding:5px 20px;
	background:none;
	overflow:visible;
	}
.tableBasic th div.sortCurrentAscend{
	background:url(../images/dt-arrow-up.png) no-repeat right 12px;	
	}
.tableBasic th div.sortCurrentDescend{
	background:url(../images/dt-arrow-dn.png) no-repeat right 12px;	
	}			
.tableBasic th a{
	color:#036;
	font-size:11pt;
	font-weight:normal;	
	text-decoration:none;
	}
.tableBasic th a:hover{
	color:#00f;
	text-decoration:underline;
	}		
.tableBasic th a:visited{
	color:purple;
	}	
	
	
.tableBasic td{
	border-left:1px solid #7f7f7f;
	border-right:1px solid #cbcbcb;	
	}
.tableBasic td.needLine{
	border-right:1px solid #cbcbcb;	
	}		
.preferences a{
	background:url(../images/icon-link-popup.gif) no-repeat 0px 2px;
	padding-left:11px;
	margin-right:12px;
	}		
.tableBasic td a,tableBasic td a.popupWindow,.preferences a{
	color:#00f;
	}	
.tableBasic td a:hover,.preferences a:hover{
	color:#6565ff;
	}
.tabBasic td a.popupWindow{
	padding-left:11px;
	background:url(../images/icon-link-popup.gif) no-repeat 0px 2px;	
	}				
.tabBasic td select{
	font-size:11.2px;
	float:right;
	width:86px;
	border:1px solid #999;
	background:#FFFFE6;
	}
.tabBasic td select.special{
	float:none;
	width:auto;
	background:#FFF;
	}			
.tableBasic tr.bgcolorType2{
	background:none;
	}	
.tableBasic tr.bgcolorType1{
	background:#edf5ff;
	}	
.tableBasic tr.bgcolorType-selected{
	background:#ffff4c;
	}
.tableBasic tr td.columnHighLight1{
	background:#dbeaff;
	}	
.tableBasic tr td.columnHighLight2{
	background:#edf5ff;
	}
.tableBasic td.bottomTD{
	background:#FFF; 
	width:100%; 
	border-left:none;  
	border-top:1px solid #7f7f7f;
	}	
.tableBasic td.topTD{	
	background:#036; 
	width:100%; 
	color:#FFF;
	}

.createContract{
	max-width:100%;
	width:100%;	
	}

.tableGridForm{
	overflow:visible;
	}
