package com.platform.gui.framework.smart.tags.model;

import java.util.List;

import com.platform.gui.framework.smart.SmartConstants;

public class SmartSelect extends SmartElement{
	
	private List<SmartOption> options;
	
	public SmartSelect(){
		this.dTemplate = SmartConstants.TEMPLATE_SELECT;
	}
	
	public List<SmartOption> getOptions() {
		return options;
	}

	public void setOptions(List<SmartOption> options) {
		this.options = options;
	}		
}
