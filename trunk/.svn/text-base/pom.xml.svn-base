<?xml version="1.0" encoding="UTF-8"?>

<project>
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.platform.gui.framework.smart</groupId>
	<artifactId>smarttag-framework</artifactId>
	<version>1.1-SNAPSHOT</version>
	
	<packaging>pom</packaging>
	<name>Smart Tag</name>
	
	<properties>
		<smarttag.jar.version>1.5.3</smarttag.jar.version>
		<smarttag.war.version>1.5.3</smarttag.war.version>
	</properties>
	
	<modules>
		<module>jar</module>
		<module>plugins/struts2</module>
		<module>web</module>
	</modules>
	
	<build>
	  <finalName>smarttag</finalName>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
					<configuration>
						<source>1.5</source>
						<target>1.5</target>
					</configuration>
			</plugin>
			<plugin>
				<groupId>org.mortbay.jetty</groupId>
				<artifactId>maven-jetty-plugin</artifactId>
				<version>6.1.5</version>
				<configuration>
					<scanIntervalSeconds>10</scanIntervalSeconds>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<dependencies></dependencies>
</project>
