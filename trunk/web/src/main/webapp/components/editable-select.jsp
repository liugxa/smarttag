<%@ taglib prefix="s" uri="/struts-tags" %>
<%@ taglib prefix="pf" uri="http://www.platform.com/tags" %>

<html>
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<link href="./css/dropdown.css" rel="stylesheet" type="text/css"/>
</head>
	
<title>Smart Tag Example</title>

<body>
	pf:property:
	<pf:property value="AAAAAA 					CCCC"/>
	
	</p>
	
	pf:select:<br>
		&lt;pf:select  list="#{'01':'Jan', '02':'Feb'}" id="id0" name="name0"/&gt;<br> 
		<pf:select  list="#{'01':'Jan', '02':'Feb'}" id="0" name="name0"/>
		
		<br>
		&lt;pf:select  list="list" id="id1" name="name1"/&gt;<br>
		<pf:select  list="list" id="1" name="name1"/>
		
		<br>
		&lt;pf:select  list="options" id="id2" name="name2" listKey="name" listValue="value"/&gt;<br>
		<pf:select  list="options" id="2" name="name2" listKey="name" listValue="value"/>
		
		
		<br>
		&lt;pf:select  list="map" id="id3" name="name3"/&gt;<br>
		<pf:select  list="map" id="3" name="name3"/>
		<pf:select  list="#{'01':'Jan', '02':'Feb', '03':'Feb', '04':'Feb', '05':'Feb', '06':'Feb'}" id="4" name="name4"/>
</body>
</html>