<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC 
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="request" namespace="/request" extends="struts-smart">
		<action name="toList" class="com.platform.gui.demo.request.action.RequestAction" method="toList">
			<result>list.jsp</result>
		</action>
		
		<action name="toItemList" class="com.platform.gui.demo.request.action.RequestItemAction" method="toList">
			<result>list-item.jsp</result>
		</action>

		<action name="toMultiList" class="com.platform.gui.demo.request.action.RequestMultiListAction" method="toMultiList">
			<result>list-multi.jsp</result>
		</action>			
	</package>
</struts>
