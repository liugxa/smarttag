package com.platform.gui.framework.tags.model;

public class SmartButton {

	private String name;
	
	private String label;

	public SmartButton(){}
	
	public SmartButton(String label , String name){
		this.label = label;
		this.name = name;
	}
	
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getLabel() {
		return label;
	}

	public void setLabel(String label) {
		this.label = label;
	}
}
