package com.platform.gui.framework.tags.decorate;

public class DefaultColumnDecorate implements IColumnDecorate{

	private String pattern;
	
	public DefaultColumnDecorate(){}
	
	public DefaultColumnDecorate(String pattern){
		this.pattern = pattern;
	}
	
	public String decorate(Object column) throws Exception {
		return column.toString();
	}

	public String getPattern() {
		return pattern;
	}

	public void setPattern(String pattern) {
		this.pattern = pattern;
	}
}
