<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC 
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="chart" namespace="/chart" extends="struts_base">
		<action name="allCharts" class="com.platform.gui.demo.chart.action.ApplicationChartAction" method="allCharts">
			<result name="success">/jsp/demo/chart/charts.jsp</result>
		</action>
		
		<action name="toChart" class="com.platform.gui.demo.chart.action.ApplicationChartAction" method="toChart">
			<result name="success">/jsp/demo/chart/application.jsp</result>
		</action>
		<action name="getChart" class="com.platform.gui.demo.chart.action.ApplicationChartAction" method="doChart"/>
	</package>
</struts>
